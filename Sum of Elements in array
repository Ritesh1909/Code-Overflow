#include <iostream> 
using namespace std; 
  

int sum(int arr[], int n)  
{  
    int sum = 0; 
  
    for (int i = 0; i < n; i++)  
    sum = sum + arr[i];  
  
    return sum;  
}  
  

int main()  
{  
    int Array1[] = {99, 99, 99, 99};  
    int N = sizeof(Array1) / sizeof(Array1[0]);  
    cout << "Sum of given array is " << sum(Array1, N);  
    return 0;  
}  
